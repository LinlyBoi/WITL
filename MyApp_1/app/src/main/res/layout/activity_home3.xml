<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".begin_activity">

    <androidx.core.widget.NestedScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/page_getinh_started_ek1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">




                <RelativeLayout android:id="@+id/page_choose_ek1"
                    android:layout_marginTop="0dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >


                    <View
                        android:id="@+id/_bg__choose_ek2"
                        android:layout_width="426dp"
                        android:layout_height="926dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:background="@drawable/_bg__choose_ek2_shape" />

                    <ImageView
                        android:id="@+id/train_image"
                        android:layout_width="500dp"
                        android:layout_height="460dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="-20dp"
                        android:contentDescription="train_image"
                        android:src="@drawable/train_image" />

                    <RelativeLayout android:id="@+id/menu"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="22dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/_bg__menu_ek1"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="47dp"
                            android:layout_height="47dp" />

                        <ImageView android:id="@+id/menu_vector_3"
                            android:src="@drawable/menu_vector_3"
                            android:contentDescription="menu_vector_3"
                            android:layout_marginStart="5.88dp"
                            android:layout_marginTop="21.54dp"
                            android:layout_width="35.25dp"
                            android:layout_height="3.92dp" />

                        <ImageView android:id="@+id/menu_vector_2"
                            android:src="@drawable/menu_vector_2"
                            android:contentDescription="menu_vector_2"
                            android:layout_marginStart="5.88dp"
                            android:layout_marginTop="31.33dp"
                            android:layout_width="35.25dp"
                            android:layout_height="3.92dp" />

                        <ImageView android:id="@+id/menu_vector_1"
                            android:src="@drawable/menu_vector_1"
                            android:contentDescription="menu_vector_1"
                            android:layout_marginStart="5.88dp"
                            android:layout_marginTop="11.75dp"
                            android:layout_width="35.25dp"
                            android:layout_height="3.92dp" />

                    </RelativeLayout>

                    <TextView android:id="@+id/good_evening___"
                        android:textStyle="bold"
                        android:textSize="24sp"
                        android:layout_marginLeft="29dp"
                        android:layout_marginTop="90dp"
                        android:layout_width="230.4dp"
                        android:layout_height="67.4dp"
                        android:text="@string/good_evening____string"
                        />


                    <RelativeLayout android:id="@+id/arrival_group"
                        android:layout_marginTop="520dp"
                        android:layout_marginStart="24dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/_bg__arrival_group_ek1"
                            android:background="@drawable/_bg__arrival_group_ek1_shape"
                            android:layout_marginStart="-24dp"
                            android:layout_marginTop="-670dp"
                            android:layout_width="380dp"
                            android:layout_height="53dp" />


                        <RelativeLayout android:id="@+id/arrival_carton"
                            android:layout_marginTop="0dp"
                            android:layout_marginStart="0dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                            <View   android:id="@+id/_bg__arrival_carton_ek1"
                                android:layout_marginStart="0dp"
                                android:layout_marginTop="0dp"
                                android:layout_width="50dp"
                                android:layout_height="50dp" />

                            <ImageView android:id="@+id/arrval_bus_icon"
                                android:src="@drawable/arrval_bus_icon"
                                android:contentDescription="arrval_bus_icon"
                                android:layout_marginStart="9.38dp"
                                android:layout_marginTop="4.69dp"
                                android:layout_width="31.25dp"
                                android:layout_height="40.63dp" />

                        </RelativeLayout>

                        <TextView android:id="@+id/arrival_time_"
                            android:textStyle="bold"
                            android:textSize="18sp"
                            android:layout_marginLeft="59dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="121.8dp"
                            android:layout_height="23.8dp"
                            android:text="@string/arrival_time__string"
                            />

                        <TextView android:id="@+id/victoria"
                            android:textSize="18sp"
                            android:layout_marginLeft="265dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="76.8dp"
                            android:layout_height="23.8dp"
                            android:text="@string/victoria_string"
                            />

                        <TextView android:id="@+id/predicted_time"
                            android:textSize="18sp"
                            android:layout_marginLeft="80dp"
                            android:layout_marginTop="34dp"
                            android:layout_width="137.8dp"
                            android:layout_height="23.8dp"
                            android:text="@string/predicted_time_string"
                            />


                        <RelativeLayout android:id="@+id/arrival_carton_ek2"
                            android:layout_marginTop="33dp"
                            android:layout_marginStart="57dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                            <View   android:id="@+id/_bg__arrival_carton_ek3"
                                android:layout_marginStart="0dp"
                                android:layout_marginTop="0dp"
                                android:layout_width="21dp"
                                android:layout_height="20dp" />

                            <ImageView android:id="@+id/arrival_carton_icon"
                                android:src="@drawable/arrival_carton_icon"
                                android:contentDescription="arrival_carton_icon"
                                android:layout_marginStart="3.42dp"
                                android:layout_marginTop="3dp"
                                android:layout_width="15dp"
                                android:layout_height="14dp" />

                        </RelativeLayout>

                        <View   android:id="@+id/arrival_vector_ellipse"
                            android:background="@drawable/arrival_vector_ellipse_shape"
                            android:layout_marginStart="350dp"
                            android:layout_marginTop="10dp"
                            android:layout_width="30dp"
                            android:layout_height="30dp" />

                        <ImageView android:id="@+id/arrival_vector"
                            android:src="@drawable/arrival_vector"
                            android:contentDescription="arrival_vector"
                            android:layout_marginStart="360dp"
                            android:layout_marginTop="16dp"
                            android:layout_width="10.21dp"
                            android:layout_height="17.5dp" />

                    </RelativeLayout>

                </RelativeLayout>
            </RelativeLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>