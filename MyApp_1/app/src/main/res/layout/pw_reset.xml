<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".pw_reset_activity">

	<androidx.core.widget.NestedScrollView
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:fillViewport="true"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content">

			<RelativeLayout
				android:id="@+id/page_getinh_started_ek1"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent">

				<RelativeLayout android:id="@+id/page_pw_reset_ek1"
					android:layout_marginTop="0dp"
					android:layout_marginStart="0dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					>

					<View   android:id="@+id/_bg__pw_reset_ek2"
						android:background="@drawable/_bg__pw_reset_ek2_shape"
						 android:layout_marginStart="0dp"
						android:layout_marginTop="0dp"
						android:layout_width="428dp"
						android:layout_height="926dp" />

					<View   android:id="@+id/rectangle_38"
						android:background="@drawable/rectangle_38_shape"
						 android:layout_marginStart="28dp"
						android:layout_marginTop="320dp"
						android:layout_width="372dp"
						android:layout_height="60dp" />

					<TextView android:id="@+id/enter_the_email_associated_with_your_account_"
						android:textStyle="bold"
						android:textSize="18sp"
						android:textColor="@color/_bg__iphone_status_bar_ek1_color"
						android:layout_marginLeft="28dp"
						android:layout_marginTop="157dp"
						android:layout_width="323.8dp"
						android:layout_height="48.8dp"
						android:text="@string/enter_the_email_associated_with_your_account__string"
						 />

					<TextView android:id="@+id/email_address"
						android:textSize="18sp"
						android:textColor="@color/_bg__iphone_status_bar_ek1_color"
						android:layout_marginLeft="28dp"
						android:layout_marginTop="273dp"
						android:layout_width="128.8dp"
						android:layout_height="25.8dp"
						android:text="@string/email_address_string"
						 />

					<TextView android:id="@+id/abcd_gmail_com"
						android:textSize="18sp"
						android:textColor="@color/_bg__iphone_status_bar_ek1_color"
						android:layout_marginLeft="60dp"
						android:layout_marginTop="339dp"
						android:layout_width="175.8dp"
						android:layout_height="32.8dp"
						android:text="@string/abcd_gmail_com_string"
						 />


					<RelativeLayout android:id="@+id/group_10"
						android:layout_marginTop="790dp"
						android:layout_marginStart="272dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						>

						<View   android:id="@+id/_bg__group_10_ek1"
							android:background="@drawable/_bg__group_10_ek1_shape"
							 android:layout_marginStart="-272dp"
							android:layout_marginTop="-790dp"
							android:layout_width="128dp"
							android:layout_height="60dp" />

						<View   android:id="@+id/rectangle_1"
							android:background="@drawable/rectangle_1_shape"
							 android:layout_marginStart="0dp"
							android:layout_marginTop="0dp"
							android:layout_width="128dp"
							android:layout_height="60dp" />

						<TextView android:id="@+id/send"
							android:textStyle="bold"
							android:textSize="22sp"
							android:textColor="@color/_bg__pw_reset_ek2_color"
							android:layout_marginLeft="38dp"
							android:layout_marginTop="6dp"
							android:layout_width="77.2dp"
							android:layout_height="57.2dp"
							android:text="@string/send_string"
							 />

					</RelativeLayout>

					<TextView android:id="@+id/reset_password"
						android:textStyle="bold"
						android:textSize="22sp"
						android:textColor="@color/rectangle_1_color"
						android:layout_marginLeft="49dp"
						android:layout_marginTop="70dp"
						android:layout_width="179.2dp"
						android:layout_height="54.2dp"
						android:text="@string/reset_password_string"
						 />


					<RelativeLayout android:id="@+id/eva_arrow_ios_back_fill"
						android:layout_marginTop="80dp"
						android:layout_marginStart="17dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						>

						<View
							android:id="@+id/_bg___eva_arrow_ios_back_fill_ek1"
							android:layout_marginStart="0dp"
							android:layout_marginTop="0dp"
							android:layout_width="35dp"
							android:layout_height="30dp" />

						<ImageView android:id="@+id/vector"
							android:src="@drawable/vector"
							android:contentDescription="vector"
							 android:layout_marginStart="11.66dp"
							android:layout_marginTop="6.25dp"
							android:layout_width="10.21dp"
							android:layout_height="17.5dp" />

					</RelativeLayout>

					<View   android:id="@+id/_home_indicator"
						android:background="@drawable/_home_indicator_shape"
						 android:layout_marginStart="155dp"
						android:layout_marginTop="915dp"
						android:layout_width="134dp"
						android:layout_height="5dp" />

				</RelativeLayout>
			</RelativeLayout>
		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>